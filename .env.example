# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
# Supabase Management API (PAT, sbp_...) for project auth config updates
SUPABASE_ACCESS_TOKEN=YOUR_SUPABASE_PERSONAL_ACCESS_TOKEN
PROJECT_REF=YOUR_PROJECT_REF

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL_LOCAL=http://localhost:3000
NEXT_PUBLIC_APP_URL_PRODUCTION=https://YOUR_PRODUCTION_DOMAIN

# Supabase Auth OAuth
# Supabase Dashboard > Authentication > URL Configuration
NEXT_PUBLIC_SUPABASE_AUTH_REDIRECT_PATH=/auth/callback

# GCP (AI generation pipeline)
GCP_PROJECT_ID=your_gcp_project_id
GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Optional APIs (Mock Media)
UNSPLASH_ACCESS_KEY=
PEXELS_API_KEY=
NEXT_PUBLIC_PEXELS_API_KEY=

# Optional AI Provider Keys
AI_PROVIDER=google
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_AI_API_KEY=

# AI Character Generation API (Next.js Route Handler)
MODEL_SIMPLE_TEXT=gemini-3-flash
MODEL_REASONING=gemini-3.1-pro
MODEL_IMAGE=nano-banana-2
MODEL_VIDEO=veo-3-fast
MODEL_AUDIO_PRIMARY=lyria-3
MODEL_AUDIO_FALLBACK=lyria-2
SUPABASE_STORAGE_BUCKET=character-generations
SUPABASE_METADATA_TABLE=character_generations
AUTOMATION_CRON_SECRET=replace-with-long-random-secret

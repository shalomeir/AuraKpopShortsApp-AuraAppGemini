# Supabase 관련 환경 변수 (https://supabase.com/dashboard/project/_/settings/api 에서 확인)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# GCP (Google Cloud Platform) 설정 (https://console.cloud.google.com/ 에서 발급)
GCP_PROJECT_ID=your_gcp_project_id
GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# AI Provider 키
## Vercel AI SDK 연동용 Provider 설정. anthropic, openai, google 중 택 1
AI_PROVIDER=anthropic
## Anthropic (Claude) API Key (https://console.anthropic.com/settings/keys 에서 발급)
ANTHROPIC_API_KEY=your_anthropic_api_key
## OpenAI API Key (https://platform.openai.com/api-keys 에서 발급)
OPENAI_API_KEY=your_openai_api_key
## Google AI Studio (Gemini) API Key (https://aistudio.google.com/app/apikey 에서 발급)
GOOGLE_AI_API_KEY=your_google_ai_api_key

# Mock 영상 데이터 조회용 (선택 항목)
## Pexels 비디오 무료 API Key (https://www.pexels.com/api/ 에서 발급)
NEXT_PUBLIC_PEXELS_API_KEY=your_pexels_api_key

# App 도메인 (Vercel 배포 후 사용)
NEXT_PUBLIC_APP_URL=http://localhost:8000
